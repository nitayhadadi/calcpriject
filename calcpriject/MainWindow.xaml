<Window x:Class="calcpriject.GamePage"
          xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="379" Width="624">
    <Grid>
        <StackPanel>
            <Label Content="What is your name?" />
            <TextBox x:Name="NameTextBox" />
            <Label Content="Age" />
            <TextBox x:Name="AgeTextBox" />
            <Button Content="submit" Click="SubmitButton_Click"   VerticalAlignment="Center"
                   />
          

            <Image Height="356" Width="634" RenderTransformOrigin="-1.862,0.531">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform AngleX="1.399"/>
                        <RotateTransform/>
                        <TranslateTransform X="-4.079"/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
        </StackPanel>
    </Grid>
</Window>
